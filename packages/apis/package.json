{
  "name": "@zk-email/zk-regex-apis",
  "version": "1.1.2",
  "description": "apis compatible with [zk-regex](https://github.com/zkemail/zk-regex/tree/main).",
  "contributors": [
    "Javier Su <javier.su.weijie@gmail.com>",
    "Kata Choi <kata.choi@gmail.com>",
    "Sora Suegami <suegamisora@gmail.com>",
    "Yush G <aayushg@mit.edu>"
  ],
  "main": "index.node",
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "node-pre-gyp install --fallback-to-build=false || npm run build-release",
    "test": "cargo test",
    "package": "cp index.node native && node-pre-gyp package",
    "upload-binary": "npm run package && node-pre-gyp-github publish --release"
  },
  "license": "MIT",
  "dependencies": {
    "cargo-cp-artifact": "^0.1",
    "node-pre-gyp": "^0.17.0"
  },
  "devDependencies": {
    "node-pre-gyp-github": "^1.4.4"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/zkemail/zk-regex/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native"
  }
}